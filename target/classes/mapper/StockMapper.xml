<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">




<!--
namespace 命名空间：应当为接口的全限定名(自JAVA目录开始)
-->
<mapper namespace="mapper.StockMapper">



    <select id="SelectAllStocks" resultType="pojo.Stock">
        select * from dbStock;
    </select>

    <select id="selectByStockName" resultType="pojo.Stock">
        select * from dbuser where db_username = #{username};
    </select>


</mapper>